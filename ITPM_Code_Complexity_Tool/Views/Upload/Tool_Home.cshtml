                <div class="content1">
                    <div class="front1">
                        Size
                    </div>
                    <div class="back1">
                        <label onclick='window.location.href = "../ComplexitySizeVariablesMethods/SetWeightSize?fileName=@fromURL"' class="button" aria-hidden="true">
                            Calculate
                        </label>
                    </div>
                </div>
            </div>
        </div>

        <div class="col-md-4">
            <div class="card1">
                <div class="content1">
