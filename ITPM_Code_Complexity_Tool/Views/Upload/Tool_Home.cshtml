@{
        Layout = null;
    }

<html>
@{
    ViewBag.Title = "Index";

    var fromURL = Request.Params["fileName"];

    if (fromURL == null)
    {
        foreach (var name in ViewBag.FILES_FROM_UPLOAD)
        {
            fromURL = name.FILE_NAME;
        }

    }

}



<head>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.16.0/umd/popper.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js"></script>
    <!-- Bootstrap CSS -->
    <link href="~/Content/style1.css" rel="stylesheet" />
    <link rel="stylesheet" href='@Url.Content("~/Content/style1.css")' asp-append-version="true" />
    <!-- Site CSS -->

</head>
<body>
    <br />
    <h1 style="color:white">Calculate Code Complexity Due To, </h1>
    <div class="row">
        <div class="col-md-4">
